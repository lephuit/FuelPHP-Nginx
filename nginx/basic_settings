##
# Nginx Basic Settings
##

sendfile on;
tcp_nopush on;
tcp_nodelay on;
keepalive_timeout 10;
types_hash_max_size 2048;
# server_tokens on;
server_tokens on;

# server_names_hash_bucket_size 64;
server_names_hash_bucket_size 64;

# Mime types
include mime.types;
default_type application/octet-stream;
